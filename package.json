{
  "name": "nuclear.js",
  "version": "0.0.3",
  "description": "Modular small front end framework",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/index.js",
    "phantom": "run-browser test/index.js -b | tap-spec",
    "browser": "run-browser test/index.js",
    "cover": "istanbul cover tape ./test/runner.js",
    "open:cover": "open coverage/lcov-report/index.html",
    "test:watch": "nodemon test/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foundersandcoders/nuclear.git"
  },
  "keywords": [
    "framework",
    "frontend",
    "virtual-dom",
    "web",
    "modular"
  ],
  "author": "Besart Hoxhaj, Wil Fisher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foundersandcoders/nuclear/issues"
  },
  "homepage": "https://github.com/foundersandcoders/nuclear",
  "dependencies": {
    "nuclear-router": "^0.0.3",
    "observ": "^0.2.0",
    "observ-array": "^3.2.1",
    "observ-struct": "^6.0.0",
    "observ-varhash": "^1.0.6",
    "xhr": "^2.0.2"
  },
  "devDependencies": {
    "function-bind": "^1.0.2",
    "istanbul": "^0.3.17",
    "opn": "^2.0.1",
    "run-browser": "^2.0.2",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "jsdom": "^5.6.1",
    "nodemon": "^1.3.8"
